@model IEnumerable<ShopOnline.Models.DBObjects.OrderDetail>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PriceTva)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Quantity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PriceTotal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdOrderNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdProductNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTvaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PriceTva)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quantity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PriceTotal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdOrderNavigation.IdOrder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdProductNavigation.IdProduct)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTvaNavigation.IdTva)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdOrderDetails">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdOrderDetails">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdOrderDetails">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
